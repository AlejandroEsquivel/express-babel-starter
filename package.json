{
  "name": "todo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/app",
    "dev": "nodemon src/app && --exec 'node -r babel-register' && npm run lint",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "echo 'No tests'",
    "lint": "esw src"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "~1.17.1",
    "config": "^1.27.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.10.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-watch": "^3.1.3",
    "npm-run-all": "^4.1.1"
  }
}
